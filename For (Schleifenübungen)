public partial class frmFor_Ue21 : Form
    {
        public frmFor_Ue21()
        {
            InitializeComponent();
        }

        private void cmdVor_Click(object sender, EventArgs e)
        {
            Int16 intEintrag;
            for(intEintrag = 1; intEintrag <= 10; intEintrag++)
            {
                // Liste füllen
                this.lstAusgabe.Items.Add("Eintrag " + intEintrag);
            }
        }

        private void cmdRueck_Click(object sender, EventArgs e)
        {
            Int16 intEintrag;
            for (intEintrag = 10; intEintrag >= 1; intEintrag--)
            {
                // Liste füllen
                this.lstAusgabe.Items.Add("Eintrag " + intEintrag);
            }
        }

        private void cmdLuecken_Click(object sender, EventArgs e)
        {
            Int16 intEintrag;
            for (intEintrag = 1; intEintrag < 20; intEintrag += 2)
            {
                // Liste füllen
                this.lstAusgabe.Items.Add("Eintrag " + intEintrag);
            }
        }

        private void cmdClear_Click(object sender, EventArgs e)
        {
            this.lstAusgabe.Items.Clear();
        }
    }
